{
  "@type": "Person",
  "name": "{{ .Site.Params.copyrightHolder }}",
  "email": "{{ .Site.Params.copyrightHolderEmail }}",
  {{- range .Site.Params.CopyrightRelation }}
  "{{ .relationship }}": {
    "@type": "{{ .type }}",
    "url": "{{ .url }}",
    "name": "{{ .name }}"
  },
  {{- end }}
  "sameAs": [ "{{ delimit .Site.Params.sameAs "\", \"" }}" ]
}
